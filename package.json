{
  "name": "@dyssent/fetcherjs",
  "version": "1.0.4",
  "homepage": "https://www.fetcherjs.com",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/es6/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "docs",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dyssent/fetcherjs"
  },
  "sideEffects": false,
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@svgr/webpack": "4.3.2",
    "@types/jest": "24.0.21",
    "@types/node": "12.12.5",
    "@types/react": "^16.9.2",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.0.11",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-preset-react-app": "^9.0.2",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "2.1.1",
    "dotenv": "6.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^6.1.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "3.0.2",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "http-server": "^0.12.3",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.9.0",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.0",
    "mini-css-extract-plugin": "0.8.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react": "16.13.0",
    "react-app-polyfill": "^1.0.4",
    "react-color": "^2.17.3",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.13.0",
    "react-github-btn": "^1.1.1",
    "resolve": "1.12.0",
    "resolve-url-loader": "3.1.0",
    "rollup": "^2.3.3",
    "sass-loader": "7.2.0",
    "semver": "6.3.0",
    "style-loader": "1.0.0",
    "styled-components": "^4.4.1",
    "terser-webpack-plugin": "1.4.1",
    "ts-pnp": "1.1.4",
    "typedoc": "^0.17.7",
    "typedoc-neo-theme": "^1.0.8",
    "typescript": "3.8.3",
    "uglifyjs": "^2.4.11",
    "url-loader": "2.1.0",
    "webpack": "4.41.0",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.1.1",
    "workbox-webpack-plugin": "4.3.1"
  },
  "peerDependencies": {
    "@types/react": "^16.6.0",
    "@types/react-dom": "^16.6.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test:coverage": "node scripts/test.js --coverage --watchAll=false",
    "prepublish": "npm run build:all",
    "serve:docs": "./node_modules/http-server/bin/http-server -a localhost -p 8001 ./docs",
    "build:docs": "./node_modules/typedoc/bin/typedoc --options typedoc.json ./src/fetcher && echo 'var typedoc = typedoc || {}; typedoc.search = typedoc.search || {}; typedoc.search.data = ' >> ./docs/assets/js/search.js && cat ./docs/assets/js/search.json >> ./docs/assets/js/search.js && cp ./public/favicon.ico ./docs/favicon.ico",
    "build:all": "rm -rf dist && rm -rf docs && npm run build:es6 && npm run build:esm && npm run build:cjs && npm run build:types && npm run build:docs",
    "build:types": "tsc --p tsconfig-types.json --declarationMap --outDir dist/types",
    "build:es6": "tsc --p tsconfig-lib.json --module es6 --target es6 --outDir dist/es6",
    "build:esm": "tsc --p tsconfig-lib.json --module es6 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --p tsconfig-lib.json --module commonjs --target es5 --outDir dist/cjs",
    "build:umd": "rollup dist/esm/index.js --format umd --name FetcherJS --sourcemap --output dist/umd/fetcher.js",
    "build:umd:min": "cd dist/umd && uglifyjs --compress --mangle --source-map --screw-ie8 --comments --o fetcher.min.js -- fetcher.js && gzip fetcher.min.js -c > fetcher.min.js.gz"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
